    "test": "npm run eslint && npm run ejslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/homebrew-nsisbi.git"
  },
  "keywords": [
    "nsis",
    "brew",
    "homebrew",
    "homebrew formula",
    "homebrew formulae"
  ],
  "author": "Jan T. Sott",
