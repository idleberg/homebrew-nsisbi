{
  "name": "homebrew-nsisbi",
  "version": "0.0.0",
  "description": "Homebrew tap for NSISBI, a fork of Nullsoft Scriptable Install System with support for big installers",
  "private": true,
  "scripts": {
    "build": "node cmd/build.js",
    "ejslint": "ejslint ./cmd/**/*.ejs",
    "eslint": "eslint ./cmd/**/*.js",
