  "bugs": {
    "url": "https://github.com/idleberg/homebrew-nsisbi/issues"
  },
  "homepage": "https://github.com/idleberg/homebrew-nsisbi#readme",
  "dependencies": {},
  "devDependencies": {
    "download": "^7.1.0",
    "ejs": "^2.6.1",
    "ejs-lint": "^0.3.0",
    "es6-promise": "^4.2.4",
    "eslint": "^5.4.0",
    "hasha": "^3.0.0",
    "husky": "^1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "log-symbols": "^2.2.0"
  }
